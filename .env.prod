# Production Environment Variables
# Copy this file to .env in production and fill in your actual values

# Database Configuration
DATABASE_URL="postgresql://USER:PASSWORD@HOST:5432/DATABASE_NAME?schema=public"

# NextAuth Configuration
NEXTAUTH_URL="https://your-domain.com"
NEXTAUTH_SECRET="your-production-secret-here-use-openssl-rand-base64-32"

# API Keys (OpenAI, Deepseek, Seedream, etc.)
# Add your production API keys here
# OPENAI_API_KEY=""
# DEEPSEEK_API_KEY=""
# SEEDREAM_API_KEY=""

# Application Settings
NODE_ENV="production"
PORT=3000
